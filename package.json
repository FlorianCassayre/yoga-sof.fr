{
  "name": "yoga-sof",
  "version": "0.2.4",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "generate-favicons": "node scripts/favicons.js",
    "lint": "next lint",
    "lint-fix": "eslint --fix 'components/**/*.js' 'hooks/**/*.js' 'lib/**/*.js' 'pages/**/*.js' 'state/**/*.js' '*.js'",
    "prisma-migrate-dev": "npx dotenv -e .env.development -- npx prisma migrate dev; npx prisma generate",
    "start-production": "npx next start -p 8085 -H localhost",
    "prisma-migrate-production": "npx dotenv -e .env.production -- npx prisma migrate deploy; npx prisma generate",
    "backup-production": "(export $(cat .env.production | xargs) && mysqldump $DATABASE_NAME -u $DATABASE_USER -p$DATABASE_PASSWORD > \"backups/$(date +\"%Y%m%d%H%M%S\").sql\")",
    "update": "git pull --ff-only && npm ci && npm run backup-production && npm run prisma-migrate-production && npm run build && echo 'Done. Start with: npm run start-production'"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "dependencies": {
    "@fontsource/nunito": "^4.5.9",
    "@mdx-js/loader": "^2.1.2",
    "@mdx-js/react": "^2.1.2",
    "@next-auth/prisma-adapter": "^1.0.3",
    "@next/mdx": "^12.1.0",
    "@premieroctet/next-crud": "^2.2.0",
    "@prisma/client": "^3.15.1",
    "@syncfusion/ej2-react-maps": "^19.4.42",
    "bootstrap": "^5.1.3",
    "date-fns": "^2.28.0",
    "final-form": "^4.20.6",
    "ics": "^2.35.0",
    "joi": "^17.5.0",
    "next": "^12.1.6",
    "next-auth": "^4.6.1",
    "nodemailer": "^6.7.5",
    "react": "17.0.2",
    "react-bootstrap": "^2.4.0",
    "react-datepicker": "^4.8.0",
    "react-dom": "17.0.2",
    "react-final-form": "^6.5.9",
    "react-final-form-listeners": "^1.0.3",
    "react-icons": "^4.4.0"
  },
  "devDependencies": {
    "dotenv-cli": "^5.1.0",
    "eslint": "^8.17.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "12.1.5",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "favicons": "7.0.0-beta.3",
    "prisma": "^3.15.1"
  }
}
